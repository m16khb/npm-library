{
  "name": "@m16khb/nestjs-sidequest",
  "version": "0.3.0",
  "description": "NestJS integration for Sidequest.js - Database-native background job processing",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "nestjs",
    "sidequest",
    "background-jobs",
    "job-queue",
    "database",
    "postgresql",
    "mysql",
    "mongodb",
    "sqlite",
    "worker",
    "typescript"
  ],
  "author": "m16khb",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/m16khb/npm-library.git",
    "directory": "packages/nestjs-sidequest"
  },
  "bugs": {
    "url": "https://github.com/m16khb/npm-library/issues"
  },
  "homepage": "https://github.com/m16khb/npm-library/tree/main/packages/nestjs-sidequest#readme",
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/core": "^1.15.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "unplugin-swc": "^1.5.9",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "sidequest": "^1.13.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "rxjs": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "nestjs-cls": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=22.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
