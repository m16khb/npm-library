{
  "name": "@m16khb/nestjs-traceable",
  "version": "1.0.0",
  "description": "NestJS용 Traceable 데코레이터 - 메서드 실행 추적 및 로깅 라이브러리",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./decorators": {
      "types": "./dist/decorators/index.d.ts",
      "import": "./dist/decorators/index.js"
    },
    "./interceptors": {
      "types": "./dist/interceptors/index.d.ts",
      "import": "./dist/interceptors/index.js"
    },
    "./interfaces": {
      "types": "./dist/interfaces/index.d.ts",
      "import": "./dist/interfaces/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "nestjs",
    "decorator",
    "traceable",
    "tracing",
    "logging",
    "interceptor",
    "aop",
    "observability",
    "monitoring",
    "typescript"
  ],
  "author": "m16khb",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/m16khb/npm-library.git",
    "directory": "packages/nestjs-traceable"
  },
  "bugs": {
    "url": "https://github.com/m16khb/npm-library/issues"
  },
  "homepage": "https://github.com/m16khb/npm-library/tree/main/packages/nestjs-traceable#readme",
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "rxjs": "^7.0.0",
    "nestjs-cls": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": false
    },
    "@nestjs/core": {
      "optional": false
    },
    "rxjs": {
      "optional": false
    },
    "nestjs-cls": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}