{
  "name": "@m16khb/nestjs-traceable",
  "version": "1.3.0",
  "description": "NestJS용 Traceable 데코레이터 - 메서드 실행 추적 및 로깅 라이브러리",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "nestjs",
    "decorator",
    "traceable",
    "tracing",
    "logging",
    "interceptor",
    "aop",
    "observability",
    "monitoring",
    "typescript"
  ],
  "author": "m16khb",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/m16khb/npm-library.git",
    "directory": "packages/nestjs-traceable"
  },
  "bugs": {
    "url": "https://github.com/m16khb/npm-library/issues"
  },
  "homepage": "https://github.com/m16khb/npm-library/tree/main/packages/nestjs-traceable#readme",
  "devDependencies": {
    "@nestjs/bullmq": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "10",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/core": "^1.15.3",
    "@types/express": "^5.0.6",
    "@types/supertest": "^6.0.3",
    "bullmq": "^5.0.0",
    "nest-winston": "^1.10.2",
    "nestjs-cls": "^4.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0",
    "supertest": "^7.1.4",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "unplugin-swc": "^1.5.9",
    "vitest": "^2.0.0",
    "winston": "^3.17.0",
    "dayjs": "^1.11.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "@nestjs/bullmq": "^10.0.0 || ^11.0.0",
    "@nestjs/schedule": "^4.0.0 || ^5.0.0",
    "bullmq": "^5.0.0",
    "nestjs-cls": "^4.0.0 || ^5.0.0",
    "nest-winston": "^1.9.0 || ^2.0.0",
    "winston": "^3.0.0",
    "dayjs": "^1.11.0",
    "rxjs": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/bullmq": {
      "optional": true
    },
    "@nestjs/schedule": {
      "optional": true
    },
    "bullmq": {
      "optional": true
    },
    "nest-winston": {
      "optional": true
    },
    "winston": {
      "optional": true
    },
    "dayjs": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
